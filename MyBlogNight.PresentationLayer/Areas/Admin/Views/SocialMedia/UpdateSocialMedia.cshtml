@model SocialMedia

@{
    ViewData["Title"] = "UpdateSocialMedia";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<div class="col-lg-12">

    <form method="post">
        <input type="hidden" asp-for="SocialMediaID" />
        <br />
        <label>Başlık</label>
        <input type="text" asp-for="Title" class="form-control" />

        <br />
        <label>IconUrl</label>
        <input type="text" asp-for="IconUrl" class="form-control" />
        <br />
        <label>LinkUrl</label>
        <input type="text" asp-for="LinkUrl" class="form-control" />
        <br />

        <button class="btn btn-primary">Sosyal Medya Güncelle</button>
    </form>

</div>

